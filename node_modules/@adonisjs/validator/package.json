{
  "name": "@adonisjs/validator",
  "version": "5.0.6",
  "description": "Validator for adonis framework",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc node japaFile.js",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "commit": "git-cz",
    "lint": "standard"
  },
  "author": "virk",
  "license": "MIT",
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "indicative": "^5.0.8",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/mrm-preset": "^1.0.14",
    "@adonisjs/sink": "^1.0.17",
    "clone": "^2.1.2",
    "commitizen": "^3.0.2",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^2.0.5",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^13.1.0",
    "pkg-ok": "^2.3.1",
    "standard": "^12.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-validation-provider.git"
  },
  "keywords": [
    "validator",
    "schema-validation",
    "adonis"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-validation-provider/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-validation-provider#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  }
}
